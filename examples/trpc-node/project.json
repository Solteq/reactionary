{
  "name": "trpc-node",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/trpc-node/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/examples/trpc-node",
        "format": ["esm"],
        "bundle": true,
        "main": "examples/trpc-node/src/main.ts",
        "tsConfig": "examples/trpc-node/tsconfig.app.json",
        "assets": ["examples/trpc-node/src/assets"],
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [
        "build"
      ],
      "options": {
        "buildTarget": "trpc-node:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "trpc-node:build:development"
        },
        "production": {
          "buildTarget": "trpc-node:build:production"
        }
      },
      "continuous": true
    }
  }
}
